{
  "name": "webpack-exercise-apptension",
  "version": "1.0.0",
  "description": "",
  "main": "webpack.dev.js",
  "scripts": {
    "start": "npm-run-all clean-dist start-dev-server",
    "build:dev": "npm-run-all clean-dist webpack:dev",
    "build:prod": "npm-run-all clean-dist webpack:prod",
    "clean-dist": "rimraf ./dist/*",
    "webpack:dev": "NODE_ENV=dev webpack --config webpack-dev.config.babel.js",
    "webpack:prod": "NODE_ENV=prod webpack --config webpack-prod.config.babel.js",
    "start-dev-server": "NODE_ENV=dev babel-node webpack-dev-server.js"
  },
  "dependencies": {
    "lodash": "^4.17.2"
  },
  "devDependencies": {
    "autoprefixer": "^6.7.0",
    "babel": "^6.5.2",
    "babel-cli": "6.18.0",
    "babel-core": "^6.22.1",
    "babel-loader": "^6.2.10",
    "babel-polyfill": "^6.7.4",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-react": "^6.5.0",
    "babel-register": "6.18.0",
    "css-loader": "^0.26.1",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.9.0",
    "html-webpack-plugin": "^2.26.0",
    "json-loader": "^0.5.4",
    "node-sass": "^4.3.0",
    "npm-run-all": "^4.0.1",
    "postcss-loader": "^1.2.2",
    "rimraf": "^2.5.4",
    "sass-loader": "^4.1.1",
    "webpack": "^1.14.0",
    "webpack-dev-server": "^1.16.2",
    "webpack-spritesmith": "^0.3.1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mikeqcp/webpack-exercise-apptension.git"
  },
  "author": "Apptension"
}
